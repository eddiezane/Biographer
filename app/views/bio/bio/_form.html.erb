<%= form_for :bio, :url => { :action => "create" } do |f| %>
  <%= f.label 'first_name' %>
  <%= f.text_field 'first_name' %>
  <%= f.label 'last_name' %>
  <%= f.text_field 'last_name' %>
  <%= f.label 'phone_number' %>
  <%= f.text_field 'phone_number' %>
  <%= f.label 'school' %>
  <%= f.text_field 'school' %>
  <%= f.label 'occupation' %>
  <%= f.text_field 'occupation' %>
  <%= f.label 'resume_url' %>
  <%= f.text_field 'resume_url' %>
  <%= f.submit %>
<% end %>
